{
	"extends": "./tsconfig.build.json",
	"include": ["src/**/*.ts", "src/**/*.d.ts", "dump-model.ts"],
	"exclude": ["node_modules/**", "@getontime-types"],
	"compilerOptions": {
		"sourceMap": true,
		"declaration": true,
		"importHelpers": true,
		"types": ["node"],
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitAny": false,
	},
	"compileOnSave": false
}
